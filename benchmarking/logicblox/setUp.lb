create --unique

addblock '
  edge(src, dest) -> int(src), int(dest).
  uedge(src, dest) -> int(src), int(dest).
'

exec '
  _in(offset; src, dest) -> int(offset), int(src), int(dest).
  lang:physical:filePath[`_in] = "input.txt".
  lang:physical:fileMode[`_in] = "import".
  lang:physical:delimiter[`_in] = " ".

  +uedge(src, dest) <- _in(_; src, dest).
  +uedge(src, dest) <- _in(_; dest, src).
  +edge(src, dest) <- uedge(src, dest), src < dest.
'
