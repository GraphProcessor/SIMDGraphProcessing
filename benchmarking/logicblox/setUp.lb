create --unique

addblock '
  edge(src, dest) -> int(src), int(dest).
'

exec '
  _in(offset; src, dest) -> int(offset), int(src), int(dest).
  lang:physical:filePath[`_in] = "input.csv".
  lang:physical:fileMode[`_in] = "import".
  lang:physical:delimiter[`_in] = ",".

  +edge(src, dest) <- _in(_; src, dest).
'
